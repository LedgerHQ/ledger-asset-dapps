{
    "name": "Paraswap",
    "chainId": 1,
    "contracts": [
        {
            "name": "AugustusSwapper",
            "address": "0x1bd435f3c054b6e901b7b108a0ab7617c808677b",
            "selectors": {
                "0xec1d21dd": {
                    "name": "MegaSwap",
                    "type": "swap",
                    "defaultLabel": "Swap",
                    "options": {
                        "ethContractAddress": "0xeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeee",
                        "assertEthAmountIsZero": false
                    },
                    "arguments": [
                        {
                            "name": "fromAmount",
                            "type": "amount",
                            "format": "amount",
                            "label": "Sent amount",
                            "value": {
                                "quantity": {
                                    "defaultPath": "data.fromAmount"  // (0)(1),
                                    "cryptoPath": "__transaction__.amount"
                                },
                                "tokenPath": "data.fromToken"  // (0)(0)
                            }
                        },
                        {
                            "name": "toAmount",
                            "type": "amount",
                            "format": "amount",
                            "label": "Received amount",
                            "value": {
                                "quantity": {
                                    "defaultPath": "data.toAmount"  // (0)(2),
                                },
                                "tokenPath": "data.path.0.path.-1.to"  // (0)(7)[0](1)[-1](0)
                            }
                        },
                        {
                            "name": "beneficiary",
                            "type": "address",
                            "format" : "address",
                            "label" : "Beneficiary address",
                            "value": {
                                "addressPath": "data.beneficiary"  // (0)(4)
                            }
                        }
                    ]
                }
            }
        }
    ]
}
